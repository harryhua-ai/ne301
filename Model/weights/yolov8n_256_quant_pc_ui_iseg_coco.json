    "confidence_threshold": 0.5,
    "iou_threshold": 0.45,
    "max_detections": 100,
    "total_boxes": 8400,
    "mask_size": 32,
    "num_masks": 32
  },
  "runtime": {
    "execution": {
      "mode": "SYNC",
      "priority": 5,
      "timeout_ms": 5000
