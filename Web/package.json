{
  "name": "preact-app",
  "private": true,
  "version": "0.2.22.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "build:bin": "tsc -b && vite build && node static_to_c_converter.cjs -i ./dist -o ./firmware_assets && node ./scripts/pack_web_assets.cjs"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "pnpm exec eslint --quiet"
    ]
  },
  "dependencies": {
    "@lingui/core": "^5.3.3",
    "@lingui/macro": "^5.3.3",
    "@lingui/react": "^5.3.3",
    "@preact/compat": "^18.3.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/vite": "^4.1.11",
    "attr-accept": "^2.2.5",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "mockjs": "^1.1.0",
    "next-themes": "^0.4.6",
    "preact": "^10.26.9",
    "preact-compat": "^3.19.0",
    "react-router-dom": "^6.30.1",
    "sonner": "^2.0.7",
    "tailwindcss": "^4.1.11",
    "throttle-debounce": "^5.0.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@lingui/cli": "^5.3.3",
    "@lingui/swc-plugin": "^5.5.2",
    "@lingui/vite-plugin": "^5.3.3",
    "@preact/preset-vite": "^2.10.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.1.0",
    "@types/throttle-debounce": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-preact": "^0.1.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "postcss-custom-properties": "^14.0.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-logical": "^8.1.0",
    "postcss-nesting": "^13.0.2",
    "postcss-preset-env": "^10.4.0",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.5",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "^3.0.2",
